{
  "name": "webpack-cli-liujun",
  "version": "1.0.0",
  "description": "webpack 通用脚手架",
  "main": "index.js",
  "config": {
    "buildDir": "./build/",
    "devHost": "localhost",
    "devPort": 3000
  },
  "skin":"xxxx",
  "gulpConfig": {
    "copyDir": "./src/static/**",
    "copyDirBasePath": "./src/static/"
  },
  "scripts": {
    "server": "cd build & http-server -p 3000",
    "dev": "node node-dev-server ./config.dev",
    "build": "node node-webpack-build ./config.pro",
    "bie": "webpack ---watch --config ./config.pro ",
    "bug": "webpack-dev-server --config ./config.dev --hot --inline --devtool eval --progress --colors --content-base dist",
    "build2": "webpack --config ./config.pro"
  },
  "dependencies": {
    "autoprefixer": "^6.4.1",
    "avalon2": "^2.1.5",
    "babel-core": "^6.1.21",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-runtime": "^5.8.0",
    "cross-env": "^1.0.5",
    "css-loader": "^0.23.1",
    "del": "^2.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "glob": "^7.0.5",
    "gulp": "^3.9.1",
    "html-loader": "^0.4.3",
    "html-minifier": "^3.0.2",
    "http-server": "^0.9.0",
    "node-sass": "^3.8.0",
    "opn": "^4.0.2",
    "postcss": "^5.2.0",
    "postcss-loader": "^0.13.0",
    "postcss-px2rem": "^0.3.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "text-loader": "^0.0.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  },
